{
    "plugins": ["plugins/markdown", "node_modules/jsdoc-vuejs"],
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "node_modules"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
          "staticFiles": {
            "include": [
                "./demo/vue-scopedslots/dist"
            ]
          }
        },
        "systemName"            : "JFlow",
        "includeDate"           : false,
        "theme"                 : "journal"
    }
}