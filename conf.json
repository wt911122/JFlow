{
    "plugins": ["plugins/markdown", "node_modules/jsdoc-vuejs", "./jsdoc-plugin/er.js"],
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "node_modules|jsdoc-plugin|demo"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "copyright": "JFlow Copyright Â© 2021-2022 The contributors to the JFlow and JFlow projects.",
        "default": {
          "staticFiles": {
            "include": [
                "./demo/vue-scopedslots/dist",
                "./jsdoc-plugin/dist/ergraphbundle.js"
                
            ]
          }
        },
        "systemName"            : "JFlow",
        "includeDate"           : false,
        "theme"                 : "journal",
        "inverseNav"            : true,
        "footer"                : "<script>(document.body.setAttribute('style', 'font-family: -apple-system,BlinkMacSystemFont,Segoe UI,Roboto,Oxygen,Ubuntu,Cantarell,Fira Sans,Droid Sans,Helvetica Neue,sans-serif'))</script>"
    }
}